
<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>Description</td>
            <td>Price</td>
            <td>Quantity of stock</td>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in ViewBag.Items)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td class="priceAndQuantity">@item.Price Kč</td>
                <td class="priceAndQuantity">@item.QuantityOfStock</td>
            </tr>
        }
    </tbody>

</table>


